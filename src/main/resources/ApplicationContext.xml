<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
               http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="Bean wale" class="Springcore.Entity.Employee" autowire="byName">
        <property name="name" value="Guest1"/>
        <property name="id" value="101"/>
    </bean>

    <bean id="address" class="Springcore.Entity.Address">
        <property name="street" value="Adarsh"/>
        <property name="city" value="Lucknow"/>
    </bean>

</beans>

